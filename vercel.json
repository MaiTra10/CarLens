{
    "name": "car-lens",
    "builds": [
        {
            "src": "car-lens/next.config.js",
            "use": "@vercel/next"
        }
    ],
    "ignore": [
        "api/*",
        "TestMockAPI/*",
        "AITestAPI/*",
        "README.md"
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "car-lens/$1"
        }
    ]
}